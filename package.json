{
  "name": "job-listing",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "6.9.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
