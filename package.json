    "lodash": "^4.17.10",
    "material-ui": "^0.20.0",
    "node-sass-chokidar": "^1.1.2",
    "npm-run-all": "^4.1.2",
    "react": "^16.3.1",
    "react-addons-css-transition-group": "^15.6.2",
    "react-dom": "^16.3.1",
    "react-firebase-file-uploader": "^2.4.1",
    "react-fontawesome": "^1.6.1",
    "react-google-autocomplete": "^1.0.15",
    "react-html-converter": "^0.3.2",
    "react-image-carousel": "^1.0.17",
    "react-image-upload": "0.0.1",
    "react-images-uploader": "^1.2.0-rc1",
    "react-infinite-scroller": "^1.1.4",
    "react-notifications": "^1.4.3",
    "react-notify-toast": "^0.4.0",
    "react-progress-button": "^5.1.0",
    "react-redux": "^5.0.7",
    "react-responsive-carousel": "^3.1.37",
    "react-route-guard": "^1.0.7",
    "react-router-dom": "^4.2.2",
    "react-s-alert": "^1.4.1",
    "react-scripts": "1.1.4",
    "redux": "^3.7.2",
    "redux-persist": "^5.9.1",
    "redux-thunk": "^2.2.0",
    "uuid": "^3.2.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
